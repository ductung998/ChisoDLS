            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#Trangchu">Mục đích</a></li>
                <li><a data-toggle="tab" href="#menu1">Ứng dụng</a></li>
                <li><a data-toggle="tab" href="#menu2">Phương pháp</a></li>
                <li><a data-toggle="tab" href="#menu3">Tài liệu tham khảo</a></li>
            </ul>
            <div class="tab-content">
                <div id="Trangchu" class="tab-pane fade in active">
                    <p>
                        <textarea class="tab-textarea" style="white-space: pre-line; width: 95%; height: 200px" disabled>@item.mucdich</textarea>
                    </p>
                </div>
                <div id="menu1" class="tab-pane fade">
                    <p>
                        <textarea class="tab-textarea" style="white-space: pre-line; width: 95%; height: 200px" disabled>@item.ungdung</textarea>
                    </p>
                </div>
                <div id="menu2" class="tab-pane fade">
