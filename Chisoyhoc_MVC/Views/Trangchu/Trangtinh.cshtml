                </div>
                <div id="menu3" class="tab-pane fade">
                    <p>
                        <textarea class="tab-textarea" style="white-space: pre-line; width: 95%; height: 200px" disabled>@item.tltk</textarea>
                    </p>
                </div>
            </div>
        }
