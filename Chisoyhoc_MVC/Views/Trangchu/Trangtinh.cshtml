@model Tuple<List<ClassChung.chisoyhoc>, object>

@{
    ViewBag.Title = "Trangtinh";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/Site.css" rel="stylesheet" />
    <style>
        input,
        select,
        textarea,
        button {
            max-width: 95%;
        }
    </style>
</head>
<body>
@*Tab*@
