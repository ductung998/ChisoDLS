}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>CSYH - @ViewBag.Tenchiso</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/Site.css" rel="stylesheet" />
    <style>
        input,
        select,
        textarea,
        button {
            max-width: 95%;
        }
    </style>
    @*Màu label*@
    <style>
        .custom-label {
            color: rgb(0, 187, 240); /* RGB color */
            color: #00BBF0; /* Hex color */
        }
    </style>
    <style>
        .nav-tabs {
            width: 95%;
        }
    </style>
</head>
<body>


    @*Tab*@
